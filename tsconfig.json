{
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "declaration": true,
    "strictNullChecks": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importsNotUsedAsValues": "remove",
    "target": "es2018",
    "sourceMap": true,
    "outDir": "./dist",
    "noImplicitAny": true,
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "skipLibCheck": true,
    "useUnknownInCatchVariables": false,
    "paths": {
      "@booking/*": ["./src/api-wooba-air-booking/*"],
      "@commons": ["./src/commons/index"],
      "@commons/*": ["./src/commons/*"],
      "@entrypoint/*": ["./src/entrypoint/*"],
      "@shopping/*": ["./src/api-wooba-air-shopping/*"],
      "test/*": ["./test/*"]
    }
  }
}
